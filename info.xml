<?xml version="1.0"?>
<extension key="de.systopia.crac" type="module">
  <file>crac</file>
  <name>Concurrent Resource Access Control</name>
  <description>Will issue warnings when multiple users are about to edit or access the same resource.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.crac</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.crac</url>
    <url desc="Support">https://github.com/systopia/de.systopia.crac/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-05-23</releaseDate>
  <version>0.5</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Crac</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Crac_Upgrader</upgrader>
</extension>
